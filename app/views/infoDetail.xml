<Alloy>
	<Collection src="info_urlModel">
	</Collection>
	<Window id="infoDetail" backButtonTitle="Tillbaka">
		<ScrollView class="headView" scrollType="vertical" showVerticalScrollIndicator="true">
			<View id="slideView">
				<ImageView id="infoImg">
				</ImageView>
				<View id="info">
				</View>
			</View>
			<Label class="lblInfoDetail" id="lblInfoTitle">
			</Label>
			<!-- <Label class="lbltxt" id="lblText" text="Klicka på länken nedan för att se mer information: "/> -->
			<View id="linkView">
				<TableView id="tableView" onClick="getLink">
					<TableViewRow class="row" onClick="getLink" />
				</TableView>
			</View>
		</ScrollView>
	</Window>
</Alloy>